#include <stdio.h>
//Escribe un programa que pida al usuario dos n√∫meros enteros, los sume y muestre el resultado.
int main() {
	int a,b,suma;
	printf("Ingrese primer numero\n");
	scanf ("%d", &a);
	printf("Ingrese segundo numero\n");
	scanf ("%d", &b);
	suma=a+b;
	printf("la suma del primer y segundo numero es: %d",suma);
	return 0;
}
